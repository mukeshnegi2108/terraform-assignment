{
    "agent": {
      "metrics_collection_interval": 5
    },
    "metrics": {
      "metrics_collected": {
        "disk": {
          "resources": ["/", "/tmp"],
          "measurement": ["disk_used_percent"]
        },
        "mem": {
          "measurement": ["mem_available_percent"]
        }
      },
      "aggregation_dimensions": [["InstanceId", "InstanceType"], ["InstanceId"]]
    }
  }